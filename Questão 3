// Lista de frascos
const frascos = [12, 5, 23, 17, 8, 14, 3, 19];

// Função de comparação para ordenação numérica crescente
function compararNumeros(a, b) {
    return a - b;
}

// Criar uma cópia do array original para não modificá-lo
const copiaFrascos = frascos.slice();

// Usar o método sort() para ordenar os frascos
copiaFrascos.sort(compararNumeros);

// Imprimir os frascos ordenados
console.log("Frascos ordenados:", copiaFrascos);
