// Função para verificar se um número é primo
function exibirNumerosPrimos(num) {
    if (num <= 1) {
        return false;
    }
    for (let i = 2; i <= Math.sqrt(num); i++) {
        if (num % i === 0) {
            return false;
        }
    }
    return true;
}

// Lista de números fornecidos pelo fazendeiro
const numeros = [23, 16, 11, 8, 19, 14, 5, 21];

// Array para armazenar os números primos
const primos = [];

// Verificar cada número da lista e adicionar os primos ao array 'primos'
for (let num of numeros) {
    if (exibirNumerosPrimos(num)) {
        primos.push(num);
    }
}

// Imprimir os números primos encontrados
console.log("Números primos encontrados:", primos);
